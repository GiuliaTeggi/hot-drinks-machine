  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ]
}